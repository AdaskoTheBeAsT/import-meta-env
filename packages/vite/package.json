{
  "name": "@import-meta-env/vite",
  "version": "0.1.17",
  "description": "Load environment variables into import.meta.env object",
  "license": "MIT",
  "author": "Ernest",
  "bin": {
    "import-meta-env": "bin/import-meta-env.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist bin && pnpm build-types && pnpm build-bundle && pnpm build-cli",
    "build-bundle": "esbuild src/index.ts --bundle --platform=node --target=node12 --external:vite --external:dotenv --outfile=dist/index.js",
    "build-cli": "esbuild src/cli.ts --bundle --platform=node --target=node12 --external:dotenv --outfile=bin/import-meta-env.js",
    "build-types": "tsc -p . --emitDeclarationOnly --outDir dist",
    "release": "standard-version && pnpm build",
    "test": "jest src/__tests__"
  },
  "engines": {
    "node": "^12.20.0 || >= 14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iendeavor/import-meta-env.git",
    "directory": "packages/vite"
  },
  "bugs": {
    "url": "https://github.com/iendeavor/import-meta-env/issues"
  },
  "homepage": "https://github.com/iendeavor/import-meta-env/tree/main/packages/vite#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/object-hash": "^2.2.1",
    "@types/tmp": "^0.2.3",
    "babel-jest": "^27.5.1",
    "dotenv": "^11.0.0 || ^12.0.4 || ^13.0.1 || ^14.3.2 || ^15.0.1 || ^16.0.0",
    "esbuild": "^0.14.11",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "tmp": "^0.2.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.13"
  },
  "peerDependencies": {
    "dotenv": "^11.0.0 || ^12.0.4 || ^13.0.1 || ^14.3.2 || ^15.0.1 || ^16.0.0",
    "vite": "^2.7.13"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "commander": "^9.0.0",
    "glob": "^7.2.0",
    "object-hash": "^3.0.0",
    "picocolors": "^1.0.0"
  }
}
